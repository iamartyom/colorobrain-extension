{
    "manifest_version": 2,
    "name": "Colorobrain",
    "description": "Brain game for everyone",
    "version": "0.0.1",
    "browser_action": {
        "default_popup": "html/start-page.html"
    },
    "icons": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png",
        "48": "images/logo-48.png",
        "128": "images/logo-128.png"
    },
    "permissions": [
        "storage"
    ],
    "offline_enabled": true
}